<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Poet</th>
      <th>Release<br>Year</th>
      <th>Poem</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% poems.forEach(function(m) { %>
      <tr>
        <td><%= m.title %></td>
        <td><%= m.poet %></td>
        <td><%= m.releaseYear %></td>
        <td><a href="/poems/<%= m._id %>">VIEW POEM</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>

<%- include('../partials/footer') %>